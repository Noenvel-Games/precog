STARTUP_CFLAGS =  -I/usr/include -O3 -libc -I../usr/share/boost/1.84.0/ -DNDEBUG=1 -include ../src/core/include/eon/eon.h

rule CXX_STARTUP
  command = /usr/bin/clang++ $CXX_FLAGS $STARTUP_CFLAGS -Wc++20-extensions -stdlib=libc++ -std=c++20 -I/usr/include/aarch64-linux-gnu/c++/12 -I/usr/include/aarch64-linux-gnu -I/usr/include/c++/12 -lstdc++ -o $out -c $in
  description = Building C++ object $out

rule C_STARTUP
  command = gcc $STARTUP_CFLAGS -o $out -c $in
  description = Building C object $out

rule STATIC_LIB_STARTUP
  command = $PRE_LINK && /usr/bin/ar qc $TARGET_FILE $in && /usr/bin/ranlib $TARGET_FILE && $POST_BUILD
  description = Linking static library $out
  restat = $RESTAT
